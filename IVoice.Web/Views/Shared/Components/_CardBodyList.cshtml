@model IVoice.Models.Components.CardBodyListModel

<div class="viewcard-body @Model._class" style="overflow-y: auto; @Model._style" id="@(Model._html_id != "" ? Model._html_id : "")">
    @foreach (var item in Model._lst)
    {
        <div class="viewcard-item">
            @Html.Raw(item)
        </div>
    }
</div>